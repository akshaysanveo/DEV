    <script>
        var min = 230;
        var max = 800;
        var mainmin = 200;

        $('#split-bar').mousedown(function (e) {

            e.preventDefault();
            $(document).mousemove(function (e) {
                e.preventDefault();
                $('#footernew').css("padding-left", "0px");
                var x = e.pageX - $('#sidebar').offset().left;
                if (x > min && x < max && e.pageX < ($(window).width() - mainmin)) {
                    var divwidth = document.getElementById("sidebar").offsetWidth;
                    var abc = (x - 50) - (x * 14 / 100)
                    //var abc = (x - 50);
                    $('#main').css('margin-left', abc);
                    //  el.css('margin-left', mrg + 'px');

                    //document.getElementById("main").style.marginLeft = "" + 50 + "";
                    $('#sidebar').css("width", x);
                    //  $('#main').css("margin-left", x);
                    $('#footer').css("margin-left", x);
                    var divwidth = document.getElementById("viewer").offsetWidth;
                    var divHeight = document.getElementById("split-bar").offsetHeight;

                    var MenuWidth = document.getElementById("menu").offsetWidth;
                    var sidebarWidth = document.getElementById("sidebar").offsetWidth;

                    var viewwidth = MenuWidth - sidebarWidth;

                    var viewHeight = divHeight - 70;

                    viewwidth = viewwidth + (viewwidth * 12 / 100);

                    //#akshay
                    viewHeight = viewHeight - (viewHeight * 20 / 100);
                    // document.getElementById("viewer").style.height = "700px";
                    document.getElementById("viewer").style.height = "" + viewHeight + "";
                    document.getElementById("viewer").style.width = "" + viewwidth + "";
                }
            })
        });
        $(document).mouseup(function (e) {
            //var divwidth = document.getElementById("sidebar").offsetWidth;

            //$('#main').css('margin-left', divwidth);
            $(document).unbind('mousemove');
        });


        var i = 0;
        var dragging = false;
        $('#dragbar').mousedown(function (e) {
            e.preventDefault();
            dragging = true;
            var main = $('#main');
            var ghostbar = $('<div>',
                {
                    id: 'ghostbar',
                    css: {
                        height: main.outerHeight(),
                        top: main.offset().top,
                        left: main.offset().left
                    }
                }).appendTo('body');

            $(document).mousemove(function (e) {
                ghostbar.css("left", e.pageX + 2);
            });
        });

        $(document).mouseup(function (e) {
            if (dragging) {
                var percentage = (e.pageX / window.innerWidth) * 100;
                var mainPercentage = 100 - percentage;
                $('#console').text("side:" + percentage + " main:" + mainPercentage);
                $('#sidebar').css("width", percentage + "%");
                $('#main').css("width", mainPercentage + "%");
                $('#ghostbar').remove();
                $(document).unbind('mousemove');
                dragging = false;
            }
        });


    </script>

